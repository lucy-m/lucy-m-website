{
  "name": "lucy-m-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "build": "cd fable && dotnet fable --lang typescript --outDir ../src/fable && cd .. && vite build",
    "preview": "vite preview",
    "check-svelte": "svelte-check --tsconfig ./tsconfig.json",
    "check-ts": "tsc --noEmit --project ./tsconfig.json",
    "test": "cypress open",
    "test-run-all": "cypress run --component",
    "test-percy-snapshots": "percy exec -- cypress run --component --spec '**/canvas-draw.cy.ts,**/scenes/objects/__tests__/*.cy.ts,**/spring.cy.ts,**/oscillator.cy.ts,**/TalentsOverlay.cy.ts'"
  },
  "devDependencies": {
    "@percy/cli": "^1.27.1",
    "@percy/cypress": "^3.1.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^4.0.1",
    "@types/seedrandom": "^3.0.5",
    "@types/uuid": "^9.0.2",
    "cypress": "^13.1.0",
    "fast-check": "^3.12.1",
    "svelte": "^3.57.0",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.0",
    "vite-plugin-filter-replace": "^0.1.11"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "seedrandom": "^3.0.5",
    "uuid": "^9.0.0",
    "zod": "^3.22.2"
  }
}
